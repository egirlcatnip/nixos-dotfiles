{
    "breadcrumbs.icons": false,
    "catppuccin-icons.hidesExplorerArrows": true,
    "catppuccin.accentColor": "lavender",
    "diffEditor.experimental.showMoves": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorSurroundingLines": 10,
    "editor.cursorWidth": 2,
    "editor.fontFamily": "JetBrainsMono NF, JetBrains Mono, JetBrainsMono, monospace",
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.minimap.renderCharacters": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.preview": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmPasteNative": false,
    "file-browser.hideIgnoredFiles": true,
    "file-browser.labelIgnoredFiles": true,
    "files.readonlyFromPermissions": true,
    "files.simpleDialog.enable": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "intelephense.telemetry.enabled": false,
    "nix.enableLanguageServer": true,
    "nix.serverPath": "nil",
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.diagnostics.styleLints.enable": true,
    "security.workspace.trust.enabled": false,
    "settingsSync.keybindingsPerPlatform": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "none",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.enableImages": true,
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.wordSeparators": " ()[]{}',\"`─‘’“”|_",
    "update.mode": "none",
    "window.autoDetectColorScheme": true,
    "window.commandCenter": false,
    "window.customTitleBarVisibility": "never",
    "window.dialogStyle": "custom",
    "window.menuBarVisibility": "toggle",
    "window.newWindowDimensions": "maximized",
    "window.titleBarStyle": "native",
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.editor.empty.hint": "hidden",
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "catppuccin-mocha",
    "workbench.preferredDarkColorTheme": "Adwaita Dark",
    "workbench.preferredLightColorTheme": "Adwaita Light",
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootNameShort}"
}